import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
	Scanner sc = new Scanner(System.in);
	int x = sc.nextInt();
	
	if(x+7>170){
	
	System.out.println("Yes");
	
	}
	else{
	    System.out.println("No");
	}
}
}
