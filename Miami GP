import java.util.*;
import java.lang.*;
import java.io.*;

public class Main{
	public static void main (String[] args) throws java.lang.Exception
	{
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	for(int i=0;i<n;i++){
	    int x = sc.nextInt();
	    int y = sc.nextInt();
	    
	    if(y<=1.07*x){
	        System.out.println("Yes");
	    }
	    
	    
	    else{
	        System.out.println("No");
	    }
	    
	}
	
	
	}
}
