

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		if(n>=750){
		    System.out.println("YES");
		}
		else{
		    System.out.println("NO");
		}
	}
}
